{
  "name": "react-post-viewer",
  "version": "0.0.1",
  "description": "Simple react app for viewing post relationships in a tree structure",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node -r esm ./server/app.js | pino-colada",
    "start": "npm run build && npm run serve",
    "build": "cross-env NODE_ENV=production webpack -r esm",
    "dev": "node -r esm --inspect ./server/app.js | pino-colada",
    "test": "cross-env NODE_ENV=test webpack -r esm && npm run test-runner",
    "test-runner": "node --inspect ./client/dist/test.js | faucet",
    "test-debug": "cross-env NODE_ENV=test webpack -r esm && npm run test-runner-debug",
    "test-runner-debug": "node --inspect-brk ./client/dist/test.js | faucet",
    "lint": "eslint --fix client/src/**/*.tsx"
  },
  "author": "Jake Whelan <jakewhelan22@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/enzyme": "3.10.4",
    "@types/react": "16.9.14",
    "@types/react-dom": "16.9.4",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-redux": "7.1.5",
    "@types/sinon": "7.5.1",
    "@types/tape": "4.2.33",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^2.34.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-standard-with-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "event-source-polyfill": "^1.0.15",
    "faker": "^4.1.0",
    "faucet": "0.0.1",
    "glob": "^7.1.6",
    "global-jsdom": "^6.1.0",
    "html-minifier-webpack-plugin": "^2.2.3",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.2.2",
    "jss": "^10.1.1",
    "jss-preset-default": "^10.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nanoid": "^2.1.11",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-hot-loader": "^4.12.21",
    "regenerator-runtime": "^0.13.5",
    "sinon": "^9.0.2",
    "tape": "^5.0.0",
    "tape-async": "^2.3.0",
    "terser-webpack-plugin": "^3.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
